<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ttt.tictactoegame">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".instructions" />
        <activity android:name=".instruction2" />
        <activity android:name=".instruction3" />
        <activity android:name=".aboutgame" />
        <activity android:name=".settings" />
        <activity android:name=".tilecolor" />
        <activity android:name=".startgame"></activity>
        <activity android:name=".dialog1" />
        <activity android:name=".stats" />
        <activity android:name=".scoreboard" />
        <activity android:name=".logout"></activity>
    </application>

</manifest>